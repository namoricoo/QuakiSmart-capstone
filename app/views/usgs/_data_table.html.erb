<table class="table table-striped table-bordered table-hover" id="customers">
	<thead>
		 <tr>
		   	<%  @table_header.each do |key, value| %>
		      <th><%= "#{value}" %></th>
		    <% end %>
		 </tr>				
	</thead>
  	<tbody>
  		<% @table_body.each do |hash| %>
		   	 <tr>
		   	 <%  hash.each do |key, value| %>
		   	   <% if "#{key}" == "place" %>		   	      
                  <td><%= link_to "#{value}",{ :controller => :usgs, :action => :where, :id => "#{value}"},target: '_blank'%></td>
               <% else %>
                   <td><%= "#{value}" %></td>
               <% end %>		      
		     <% end %>			      
		   	 </tr>
	    <% end %>
  	</tbody>		    		   	
</table>