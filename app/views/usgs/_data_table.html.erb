<table class="table table-striped table-bordered" id="customers">
	<thead>
		 <tr>
		   	<%  @table_header.each do |key, value| %>
		      <th><%= "#{value}" %></th>
		    <% end %>
		 </tr>				
	</thead>
  	<tbody>
  		<% @table_body.each do |hash| %>
		   	 <tr>
		   	 <%  hash.each do |key, value| %>
		      <td><%= "#{value}" %></td>
		     <% end %>			      
		   	 </tr>
	    <% end %>
  	</tbody>		    		   	
</table>