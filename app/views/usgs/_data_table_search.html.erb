<table class="table table-striped table-bordered table-hover" id="customers">
	<thead>
		 <tr>
		   	<%  @table_header.each do |key, value| %>
		      <th><%= "#{value}" %></th>
		    <% end %>
		 </tr>				
	</thead>
  	<tbody>
  		<%  @table_body.each do |earthquake| %>
	  		<tr>
	  			<td><%= earthquake.mag %></td>
	  			<td><%= link_to earthquake.place,{ :controller => :usgs, :action => :where, :id => earthquake.place }, target: '_blank'%></td>	  			
	  			<td><%= earthquake.latitude %></td>
	  			<td><%= earthquake.longitude %></td>
	  			<td><%= earthquake.tsunami %></td>
	  			<td><%= earthquake.felt %></td>
	  			<td><%= earthquake.cdi %></td>
	  			<td><%= earthquake.dmin %></td>
	  		</tr>
  		<% end %>  		
  	</tbody>		    		   	
</table>