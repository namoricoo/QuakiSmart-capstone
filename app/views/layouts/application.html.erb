</head>
<body class="ex_highlight_row">
<div id="site-container">	
	<div class="navbar navbar-fixed-top">
    <div class="navbar-inner">
      <div class="container">
        <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </a>        
        <!-- <%= link_to image_tag('logo_quakismart.png', :alt => "QuakiSmart Logo"), "http://quakismart.herokuapp.com/" %> -->
        <a class="brand" href="#">QuakiSmart</a>
        <div class="nav-collapse">
          <ul class="nav">
            <li><%= link_to "ANALYTICS" %></li>
            <li><%= link_to "ABOUT US" %></li>
            <li><%= link_to "GITHUB" %></li>           
          </ul>          
        </div>
      </div>
    </div>
  </div>	
	<div class="container">
		<%= yield %>
	</div>
	
	<footer class="quaki-footer">
	  <div class="quaki-footer-inner">
	<p><a href="#">Analytics</a> | <a href="#">About Us</a> | <a href="#">Github</a></p>
	<p>Copyright Â© 2013. Portland Code School. All Rights Reserved. Napoleon Kernessant, final Project</p>
	  </div>
	</footer>
</div>

<script src="//maps.google.com/maps/api/js?v=3.13&amp;sensor=false&amp;libraries=geometry" type="text/javascript"></script>
<script src='//google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.0.14/src/markerclusterer_packed.js' type='text/javascript'></script>
<script>
	$(document).ready(function(){	
		handler = Gmaps.build('Google');
		handler.buildMap({ provider: {}, internal: {id: 'map'}}, function(){
			markers = handler.addMarkers(<%=raw @earthquake_hash.to_json %>);
		  	handler.bounds.extendWith(markers);
		  	handler.fitMapToBounds();
		});
	});	
</script>

<script>
	  $(function() {
	    $( "#slider" ).slider();
	  });
</script>
<script>
   sliderLogic();
</script>
<script>
	dataTableFunction();
</script>
</body>
</html>
